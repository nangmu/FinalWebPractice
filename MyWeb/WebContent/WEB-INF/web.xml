<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<resource-ref>
		<description> DB Connection </description>
		<res-ref-name>jdbc/web</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
  
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>main.java.servlets.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>Register</servlet-name>
    <servlet-class>main.java.servlets.Register</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Register</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>UserPagingProceess</servlet-name>
    <servlet-class>main.java.servlets.UserPagingProcess</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserPagingProceess</servlet-name>
    <url-pattern>/userPagingProcess</url-pattern>
  </servlet-mapping>
   
   <servlet>
    <servlet-name>BoardPagingProceess</servlet-name>
    <servlet-class>main.java.servlets.BoardPagingProcess</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BoardPagingProceess</servlet-name>
    <url-pattern>/boardPagingProcess</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>LoginCheck</servlet-name>
    <servlet-class>main.java.servlets.LoginCheck</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginCheck</servlet-name>
    <url-pattern>/loginCheck</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>WriteBoard</servlet-name>
    <servlet-class>main.java.servlets.WriteBoard</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WriteBoard</servlet-name>
    <url-pattern>/writeBoard</url-pattern>
  </servlet-mapping>
 
</web-app>